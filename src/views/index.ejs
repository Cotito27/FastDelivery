  <body>
    <script>
      paceOptions = {
        catchupTime: 100,
        initialRate: 0.03,
        minTime: 250,
        ghostTime: 100,
        maxProgressPerFrame: 20,
        easeFactor: 1.25,
        startOnPageLoad: true,
        restartOnPushState: true,
        restartOnRequestAfter: 500,
        target: "body",
        elements: {
          checkInterval: 100,
          selectors: ["body"],
        },
        eventLag: {
          minSamples: 10,
          sampleCount: 3,
          lagThreshold: 3,
        },
        ajax: {
          trackMethods: ["GET"],
          trackWebSockets: true,
          ignoreURLs: [],
        },
      };
      Pace.start();
    </script>
    <script>
      let codigoMe = '<%= user.codigo %>';
    </script>
    <div class="toast-error">
      <div class="icon-error">
        <i class="fas fa-exclamation-circle"></i>
      </div>
      <div class="text-error">
        Debe ingresar
      </div>
      <div class="close-error">
        <i class="fas fa-times"></i>
      </div>
    </div>
    <div class="toast-success">
      <div class="icon-success">
        <i class="fas fa-check"></i>
      </div>
      <div class="text-success">
        Se ha registrado exitosamente.
      </div>
      <div class="close-success">
        <i class="fas fa-times"></i>
      </div>
    </div>
    
    <%- include ('partials/navbar') %> <%- include ('partials/sidebar') %>
    <script>
      let sectionGlobalPage = '<%= section %>';
    </script>
    <% if(section == 'boleta/registro') { %>
      <%- include ('partials/modal') %>
    <% } %>

    <div class="container animate__animated animate__fadeIn">
      <% if(section == 'inicio') { %> 
        <%- include ('partials/home') %> 
      <% } else if(section == 'productos/registro') { %> 
        <%- include ('partials/register_producto') %>
        <!-- <div class="separator-forms"></div>  -->
      <% } else if(section == 'productos/reporte') { %>
        <%- include ('partials/report_producto') %> 
      <% } else if(section == 'clientes/registro') { %>
        <%- include ('partials/register_cliente') %>
        <!-- <div class="separator-forms"></div>  -->
      <% } else if(section == 'clientes/reporte') { %>
        <%- include ('partials/report_cliente') %> 
      <% } else if(section == 'empleados/registro') { %>
        <%- include ('partials/register_empleado') %>
        <!-- <div class="separator-forms"></div>  -->
      <% } else if(section == 'empleados/reporte') { %>
        <%- include ('partials/report_empleado') %> 
      <% } else if(section == 'boleta/registro') { %>
        <%- include ('partials/register_boleta') %> 
      <% } else if(section.includes('registro')) { %>
        <%- include ('partials/register_sub') %> 
      <% } else if(section.includes('reporte')) { %>
        <%- include ('partials/report_sub') %> 
      <% } %>
    </div>
    <script>
      let container = document.querySelector(".container");
      let sidebar = document.querySelector(".sidebar");
      function resizeSidebar() {
        if (window.innerWidth <= 789) {
          sidebar.classList.remove("show");
          container.classList.add("all-space-width");
        } else {
          sidebar.classList.add("show");
          container.classList.remove("all-space-width");
        }
      }

      resizeSidebar();
    </script>
    <script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
